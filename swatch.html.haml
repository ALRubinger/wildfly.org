				.bar(style="width: 45%")
	%br

	/ =========================Alerts & Messages=========================
	%h3#alerts Alerts
	.row
		.span12
			.alert.alert-block
				%a.close &times;
				%h4.alert-heading Alert block
				%p Best check yo self, you're not looking too good. Nulla vitae elit libero, a pharetra augue. Praesent commodo cursus magna, vel scelerisque nisl consectetur et.
	.row
		.span4
			.alert.alert-error
				%a.close &times;
				=succeed ' Change a few things up and try submitting again.' do
					%strong Error
		.span4
			.alert.alert-success
				%a.close &times;
				=succeed ' You successfully read this important alert message.' do
					%strong Success
		.span4
			.alert.alert-info
				%a.close &times;
				=succeed " This alert needs your attention, but it's not super important." do
					%strong Information

